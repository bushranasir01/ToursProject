@model List<ToursansTripArange.Models.News>
@{
    ViewBag.Title = "shownews";
}

<div class="banner">
    <div class="agileinfo-dot">
        <div class="w3-agileits-about-banner">
            <div class="container">
                <h2>NEWS</h2>
            </div>
        </div>
    </div>
</div>


<table style="width:100%">
    <tr>

        <td>name</td>
        <td>image </td>
        <td>Description </td>
        
    </tr>
    @foreach (var s in Model)
    {
        <tr>
            <td>@s.Name</td>

            <td>@s.image</td>
            <td>@s.Description</td>
           
            <td>
               
                <a href="/Home/deletenews/@s.Id">Delete</a>
            </td>
        </tr>
    }
</table>
<a href="/Home/addnews">Insert</a>
